{
  "name": "tinytoys",
  "description": "A tiny toy to play with OpenAI API",
  "version": "1.0.0",
  "author": "Susumu Fujii",
  "dependencies": {
    "axios": "^1.8.3",
    "eslint": "^9.22.0",
    "globals": "^16.0.0",
    "openai": "^4.87.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "styled-components": "^6.1.16"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@storybook/react": "^8.6.7",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.11",
    "@types/chrome": "^0.0.309",
    "@types/lodash": "^4",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.11",
    "@types/react-dom": "^19.0.4",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "dotenv-expand": "^12.0.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "fixpack": "^4.0.0",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "prettier": "^3.5.3",
    "storybook": "^8.6.7",
    "terser": "^5.39.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "unplugin-auto-import": "^19.1.1",
    "url": "^0.11.4",
    "vite": "^6.2.2",
    "vitest": "^1.6.1"
  },
  "license": "MIT",
  "packageManager": "yarn@4.7.0",
  "private": true,
  "scripts": {
    "build": "yarn setup && vite build",
    "build-storybook": "storybook build",
    "clean": "rm -rf dist",
    "dev": "vite --host",
    "format": "prettier --write .",
    "lint": "eslint src --ext .ts,.tsx",
    "setup": "yarn setup:development",
    "setup:development": "cross-env BUILD_ENV=\"dev\" cp dotenv.development .env",
    "setup:production": "cross-env BUILD_ENV=\"production\" cp dotenv.production .env",
    "storybook": "storybook dev -p 6006",
    "test": "vitest",
    "test-storybook": "test-storybook --url http://localhost:6006",
    "test-storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"yarn build-storybook --quiet && npx http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && yarn test-storybook\"",
    "typecheck": "tsc --pretty --noEmit -p ./tsconfig.json"
  },
  "type": "module"
}
